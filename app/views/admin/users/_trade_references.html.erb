<% user = User.find(params[:id]) %>
	<table>
	<thead>
		<tr>
			<th>Company</th>
			<th>Contact</th>
			<th>Phone</th>
		</tr>
	</thead>
	<tbody>
		<% user.trade_references.each do |t| %>
		<tr>
			<td><%= t.company %></td>
			<td><%= t.contact %></td>
			<td><%= t.phone %></td>
		</tr>
		<% end %>
	</tbody>
</table>
